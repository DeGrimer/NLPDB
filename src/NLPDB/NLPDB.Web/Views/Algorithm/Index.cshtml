@model IEnumerable<NLPDB.Web.Data.Algorithm>
@{
}
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TasksAlg)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TasksAlg)
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.Id">Подробно</a>
            </td>
        </tr>
}
    </tbody>
</table>